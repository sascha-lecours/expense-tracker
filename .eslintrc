{
	"extends": "airbnb",
	"parserOptions": {
		"ecmaVersion": 6
	},
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"rules": {
		"comma-dangle": ["error", "always-multiline"],
		"react/jsx-filename-extension": 0,
		"indent":["error", "tab"],
		"no-tabs": 0,
		"no-unused-vars": ["warn", { "vars": "local", "args": "none" }]
	}
}